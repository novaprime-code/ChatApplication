    "paths": {
        "/api/chats": {
            "post": {
                "tags": [
                    "Chat"
                ],
                "summary": "Initialize Chat",
                "operationId": "b0c35d6a68b5ffdfc888e6a745da47b0",
                "requestBody": {
                    "description": "Data required to initialize a chat",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "receiver_id"
                                ],
                                "properties": {
                                    "receiver_id": {
                                        "description": "ID of the receiver user",
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Successful initialization of chat",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "id": {
                                            "description": "ID of the newly created chat",
                                            "type": "integer"
                                        },
                                        "sender_id": {
                                            "description": "ID of the sender user",
                                            "type": "integer"
                                        },
                                        "receiver_id": {
                                            "description": "ID of the receiver user",
                                            "type": "integer"
                                        },
                                        "created_at": {
                                            "description": "Timestamp when the chat was created",
                                            "type": "string",
                                            "format": "date-time"
                                        },
                                        "updated_at": {
                                            "description": "Timestamp when the chat was last updated",
                                            "type": "string",
                                            "format": "date-time"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error when required data is missing or invalid",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "description": "Validation errors",
                                            "type": "object",
                                            "example": {
                                                "receiver_id": [
                                                    "The receiver id field is required."
                                                ]
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/chats/{chatId}/messages": {
            "get": {
                "tags": [
                    "Chat"
                ],
                "summary": "Get chat history",
                "operationId": "3b1d58c3bb90861e829bcde5e429cf33",
                "parameters": [
